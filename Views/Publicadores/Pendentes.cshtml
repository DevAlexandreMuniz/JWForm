@model List<Publicador>

<div class="d-flex">
    <h1 class="flex-grow-1">
        Publicadores Pendentes
    </h1>
</div>

<hr>

<div class="list-group">
    @foreach (var item in Model)
    {
        <a class="list-group-item list-group-item-action d-flex" asp-action="Editar" asp-route-id="@item.">
            <span class='flex-grow-1'>
                @item.Nome - @item.Tipo
            </span>
        </a>
    }
</div>